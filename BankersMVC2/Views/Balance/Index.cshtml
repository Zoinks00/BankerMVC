@model IEnumerable<Balance>

<h2>Users</h2>

<table>
    <tr>
        <th>IdBalance</th>
        <th>TotalBalance</th>


    </tr>

    @foreach (var balance in Model)
    {
        <tr>
            <td>@balance.IdBalance</td>
            <td>@balance.TotalBalance</td>

        </tr>
    }

</table>
