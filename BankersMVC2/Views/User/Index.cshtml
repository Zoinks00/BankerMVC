@model IEnumerable<User>


<h2>Users</h2>

<table>
    <tr>
        <th>ID</th>
        <th>FirstName</th>
        <th>LastName</th>
       @* <th>IdBalance</th>*@


    </tr>

    @foreach (var user in Model)
    {
        <tr>
            <td><a href="/User/ViewUser/@user.IdUser">@user.IdUser</a></td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
           
            
           


        </tr>
    }

</table>
